{
  "name": "website-thingy",
  "version": "0.0.1",
  "description": "website-thingy",
  "scripts": {
    "build-styl": "stylus source/index/index.styl -o compiled/css/",
    "watch-styl": "stylus source/index/index.styl -o compiled/css/ -w",
    "build-index-pug": "pug -o output/ source/index/index.pug",
    "watch-index-pug": "pug -o output/ source/index/index.pug -w",
    "build-coffee": "coffee -o compiled/js/ -c source/*/*.coffee",
    "watch-coffee": "coffee -o compiled/js/ -cw source/*/*.coffee",
    "build-mocks-coffee": "coffee -o compiled/js/ -c mocks/*/*.coffee",
    "watch-mocks-coffee": "coffee -o compiled/js/ -cw mocks/*/*.coffee",
    "pax-bundle": "~/.cargo/bin/px -E --input compiled/js/index.js --output compiled/bundle.js",
    "watch-pax-bundle": "~/.cargo/bin/px --watch -E --input compiled/js/index.js --output compiled/bundle.js",
    "bundle": "webpack-cli --config synced-misfits/webpack.config.js",
    "watch-bundle": "webpack-cli --config synced-misfits/webpack-watch.config.js",
    "build-js": "run-s -ns build-mocks-coffee build-coffee bundle",
    "watch-js": "run-p -nsr watch-mocks-coffee watch-coffee watch-bundle",
    "build": "run-s -ns build-styl build-js build-index-pug",
    "watch": "run-p -nsr watch-js watch-styl watch-index-pug ui-sync",
    "ui-sync": "browser-sync start --server 'output/' --files 'output/index.html' --no-open",
    "test": "run-s -ns build watch",
    "initialize-project": "run-s -ns patch-stuff create-compile-folders build copyscript",
    "patch-stuff": "synced-misfits/patch-stuff.sh",
    "copyscript": "synced-misfits/copyscript.sh",
    "create-compile-folders": "synced-misfits/create-compile-folders.sh",
    "push": "synced-misfits/add-commit-and-push-all-repos.sh",
    "pull": "synced-misfits/pull-all.sh",
    "postinstall": "npm run initialize-project"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JhonnyJason/repo.git"
  },
  "author": "Lenard Frommelt",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/JhonnyJason/repo/issues"
  },
  "homepage": "https://github.com/JhonnyJason/repo#readme",
  "dependencies": {
    "browser-sync": "^2.26.3",
    "coffeescript": "^2.3.2",
    "npm-run-all": "^4.1.5",
    "pug-cli": "^1.0.0-alpha6",
    "stylus": "^0.54.5",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
